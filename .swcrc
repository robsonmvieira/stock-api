{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "keepClassNames": true,
    "paths": {
      "@services/*": ["src/services/*"],
      "@modules/*": ["src/modules/*"]
    }
  },
  "minify": false
}
